			<layout_panel name="save_changes_btn_lp">
				<button label="Guardar cambios" name="save_btn"/>
			</layout_panel>
			<layout_panel name="show_on_map_btn_lp">
				<button label="Cancelar" name="cancel_btn"/>
			</layout_panel>
		</layout_stack>
