              translate="false"
              value="http://librarianavengers.org"
              width="300"
              word_wrap="false"
              use_ellipses="true" />
            <text
             follows="left|top"
           font.style="BOLD"
             height="10"
             layout="topleft"
             left="10"
             name="title_member_text"
             text_color="white"
             top_pad="10"
             value="Resident Since:"
             width="300" />
